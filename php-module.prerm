#!/bin/sh

set -e

#EXTRA#

if [ "$1" = "remove" ] || [ "$1" = "deconfigure" ]; then
    if [ -e /usr/share/php/php@PHP_VERSION@-maintscript-helper ] ; then
	. /usr/share/php/php@PHP_VERSION@-maintscript-helper

	for dsoname in @modules@; do
	    php@PHP_MAJOR@_@PHP_MINOR@_invoke dismod ALL ${dsoname}
	done
    fi
fi

#DEBHELPER#

exit 0
