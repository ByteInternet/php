[Unit]
Description=The PHP FastCGI Process Manager
After=network.target

[Service] 
Type=notify
PIDFile=/var/run/php5.5-fpm.pid
ExecStartPre=/usr/lib/php/5.5/php5.5-fpm-checkconf
ExecStart=/usr/sbin/php5.5-fpm --nodaemonize --fpm-config /etc/php/5.5/fpm/php-fpm.conf
ExecReload=/bin/kill -USR2 $MAINPID

[Install]
WantedBy=multi-user.target
