[Unit]
Description=The PHP FastCGI Process Manager
After=network.target

[Service] 
Type=notify
PIDFile=/var/run/php5.6-fpm.pid
ExecStartPre=/usr/lib/php/5.6/php5.6-fpm-checkconf
ExecStart=/usr/sbin/php5.6-fpm --nodaemonize --fpm-config /etc/php/5.6/fpm/php-fpm.conf
ExecReload=/usr/lib/php/5.6/php5.6-fpm-checkconf
ExecReload=/bin/kill -USR2 $MAINPID

[Install]
WantedBy=multi-user.target
