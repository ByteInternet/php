#!/bin/sh

set -e

#EXTRA#

if [ "$1" = "remove" ] || [ "$1" = "deconfigure" ]; then
    if [ -e /usr/share/php5.5/php5.5-maintscript-helper ] ; then
	. /usr/share/php5.5/php5.5-maintscript-helper

	for dsoname in @modules@; do
	    php5.5_invoke dismod ALL ${dsoname}
	done
    fi
fi

#DEBHELPER#

exit 0
